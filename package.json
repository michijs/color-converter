{
  "name": "@michijs/htmltype",
  "author": "Lucas M. Segurado",
  "license": "MIT",
  "version": "0.1.14",
  "baseUrl": "./src",
  "repository": {
    "type": "git",
    "url": "https://github.com/michijs/htmltype.git"
  },
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "description": "Strict TypeScript types for HTML and SVG attributes based on MDN data",
  "scripts": {
    "dist": "tsc",
    "link": "tsc && npm link --force && tsc -w"
  },
  "keywords": [
    "html",
    "typescript"
  ],
  "devDependencies": {
    "rome": "11.0.0",
    "lint-staged": "13.1.2",
    "simple-git-hooks": "2.8.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "csstype": "3.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.ts": "rome check --apply-suggested"
  }
}
